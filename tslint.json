{
  "extends": [],
  "rules": {
    "prettier": true,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ],
    "semicolon": false
  },
  "linterOptions": {
    "exclude": ["node_modules/**", "dist/**"]
  }
}
